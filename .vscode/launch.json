  "version": "0.2.0",
  "configurations": [
  {
    "name":"Python: Main File",
    "type":"python",
    "request":"launch",
    "program":"${cwd}/Include/Python/main.py",
    "console":"integratedTerminal"
  },
  {
    "name":"Python: Current File",
    "type":"python",
    "request":"launch",
    "program":"${file}",
    "console":"integratedTerminal"
  },
  {
    "name": "Node: Current File",
    "program": "${file}",
    "request": "launch",
    "skipFiles": [
      "<node_internals>/**"
    ],
    "type": "pwa-node",
    "console": "integratedTerminal"
  },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Node: First Test Case",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/Include/Javascript/My First Javascript Test Case.js",
      "console": "integratedTerminal"
    }
  ]
}