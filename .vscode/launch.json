{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "name": "Session Manager",
      "program": "${workspaceFolder}/build/sessionManager.js",
      "request": "launch",
      "restart": true,
      "runtimeExecutable": "nodemon",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "args": [
        "--watch",
        "${workspaceFolder}/build",
        "--delay",
        "1.5"
      ],
      "type": "node"
    },
    {
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "name": "First JS test",
      "program": "${workspaceFolder}/build/firstTest.js",
      "request": "launch",
      "restart": true,
      "runtimeExecutable": "nodemon",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "args": [
        "--watch",
        "${workspaceFolder}/build",
        "--delay",
        "1.5"
      ],
      "type": "node"
    },
    {
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "name": "Current JS file",
      "program": "${file}",
      "request": "launch",
      "restart": true,
      "runtimeExecutable": "nodemon",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "args": [
        "--watch",
        "${workspaceFolder}/src",
        "--delay",
        "1.5"
      ],
      "type": "node"
    },
    {
      "name": "Python: Main File",
      "type": "python",
      "request": "launch",
      "program": "${cwd}/Include/Python/main.py",
      "console": "integratedTerminal"
    },
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    }
  ]
}